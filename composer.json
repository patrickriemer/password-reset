{
    "name": "patrickriemer/password-reset",
    "description": "CLI for resetting user password",
    "type": "library",
    "require": {
        "php": ">=7.3",
        "laravel/framework": "^6"
    },
    "autoload": {
        "psr-4": {
            "PatrickRiemer\\PasswordReset\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PatrickRiemer\\PasswordReset\\Tests\\": "tests/"
        }
    },
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Patrick Riemer",
            "email": "patrick@com1.app"
        }
    ],
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "PatrickRiemer\\PasswordReset\\PasswordResetServiceProvider"
            ]
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.2",
        "orchestra/testbench": "^5.3"
    }
}
